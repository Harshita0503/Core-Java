import java.util.*;

class test
{
public void show()
{
int a, i, fact=1;
Scanner sc=new Scanner(System.in);
System.out.println("Enter the value of a:");
a=sc.nextInt();
i=1;
do
{
fact=fact*i;
System.out.println(fact);
i++;
}
while(i<=a);
}

public static void main(String args[])
{
test t=new test();
t.show();
}
}

