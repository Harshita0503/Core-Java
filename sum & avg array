import java.util.*;
class test
{
public void show()
{
int p,sum=0,j;
float average;
Scanner sc=new Scanner(System.in);
System.out.println("enter elements:");
p=sc.nextInt();
int a[]=new int[5];
a[0]=10;
a[1]=20;
a[2]=30;
a[3]=40;
a[4]=50;
for(int i=0;i<5;i++)
{
sum=sum+a[i];
}
System.out.println(sum);
average=sum/p;
System.out.println(average);
}


public static void main(String args[])
{
test t=new test();
t.show();
}
}
