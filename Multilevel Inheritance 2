class test
{
public void show()
{
System.out.println("Hello Harshita");
}
}

class test1 extends test
{
public void show1()
{
System.out.println("Hello Sir");
}
}

class test2 extends test
{
public void show2()
{
System.out.println("Hello ma'am");
}

public static void main(String args[])
{
test1 ks=new test1();
test2 js=new test2();
ks.show1();
ks.show();
js.show2();
js.show();
}
}
