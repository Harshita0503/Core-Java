import java.util.*;
class test
{
public void show()
{
int a,i,c=1;
Scanner sc=new Scanner(System.in);
System.out.println("Enter the value of a:");
a=sc.nextInt();
i=1;
do
{
c=a*i;
System.out.println(c);
i++;
}
while(i<=10);
}

public static void main(String args[])
{
test t=new test();
t.show();
}
}






