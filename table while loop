import java.util.*;
class test
{
public void show()
{
int a, i, c=1;
Scanner sc= new Scanner(System.in);
System.out.println("Enter the value of a:");
a=sc.nextInt();
i=1;
while(i<=10)
{
c=a*i;
System.out.println(c);
i++;
}
}
public static void main(String args[])
{
test t=new test();
t.show();
}
}

